        <messages/>
        <name>Event Manager Client Script</name>
        <order/>
        <script><![CDATA[function onLoad() {
    var groupSysId = ''; // Set the Sys ID of your group here
 
    // If the user belongs to a group, set the groupSysId variable
    if (g_user.hasRole('itil') || g_user.hasRole('admin')) {
        groupSysId = 'Sys ID of Your Group'; // Replace 'Sys ID of Your Group' with the actual Sys ID of your group
    }
 
    var userField = g_form.getControl('user_field_name'); // Replace 'user_field_name' with the actual name of your user field
    if (userField) {
        userField.addQuery('group', '=', groupSysId);
        userField.setDisplayValue(userField.getValue());
    }
}
]]></script>
