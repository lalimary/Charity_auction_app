        <messages/>
        <name>Display item form  delete button </name>
        <order/>
        <script><![CDATA[function onLoad() {
    var auctionStatus = g_form.getValue('auction_status');
    var deleteButton = g_form.getControl('delete_button');

    if (auctionStatus === 'Item Submitted') {
        deleteButton.removeAttribute('disabled');
    } else {
        deleteButton.setAttribute('disabled', 'true');
    }
}
]]></script>
