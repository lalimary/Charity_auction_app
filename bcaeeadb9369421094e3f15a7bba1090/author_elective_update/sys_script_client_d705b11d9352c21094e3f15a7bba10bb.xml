        <isolate_script>true</isolate_script>
        <messages/>
        <name>NameFieldOnChange</name>
        <order/>
        <script><![CDATA[function onChange() {
    var name = g_form.getValue('name'); // Get the selected name from the reference field
    if (name) {
        var ga = new GlideAjax('RetrieveEmailScript'); // Create a GlideAjax object and specify the server-side script
        ga.addParam('sysparm_name', 'getEmail'); 
        ga.addParam('sysparm_name_value', name); // Pass the selected name as a parameter
        ga.getXMLAnswer(function(response) {
            if (response) {
                g_form.setValue('email', response); // Populate the email field with the retrieved email address
            }
        });
    }
}
]]></script>
