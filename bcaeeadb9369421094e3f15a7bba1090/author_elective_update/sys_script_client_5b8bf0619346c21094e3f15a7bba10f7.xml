        <field>end_date</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>Item end date 2</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
    var startDate = g_form.getValue('start_date');
    var endDate = g_form.getValue('end_date');

    // Convert start date and end date to Date objects
    var startDateObj = new Date(startDate);
    var endDateObj = new Date(endDate);

    // Validate end date is not before start date
    if (endDateObj < startDateObj) {
        g_form.showFieldMsg('end_date', 'End date cannot be before start date', 'error');
        g_form.clearValue('end_date');
        return;
    }

    // Validate end date is not today
    var currentDate = new Date();
    if (endDateObj.toDateString() === currentDate.toDateString()) {
        g_form.showFieldMsg('end_date', 'End date cannot be today', 'error');
        g_form.clearValue('end_date');
        return;
    }

    // Clear any existing error message
    g_form.clearMessages('end_date');
}]]></script>
