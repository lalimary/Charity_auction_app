        <messages/>
        <name>location autopopulate</name>
        <order/>
        <script><![CDATA[function onLoad() {
    // Get the current logged-in user's sys_id
    var userId = g_user.userID;

    // Create a GlideRecord object for the sys_user table
    var userGr = new GlideRecord('sys_user');
    userGr.addQuery('sys_id', userId);
    userGr.query();

    // If the user record is found
    if (userGr.next()) {
        // Get the location field value from the user record
        var location = userGr.getValue('location');

        // Set the location field value in the user profile form
        g_form.setValue('location', location);
    }
}
]]></script>
