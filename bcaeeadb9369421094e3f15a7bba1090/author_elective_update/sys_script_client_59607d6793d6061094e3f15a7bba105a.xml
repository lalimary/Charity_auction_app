        <name>script4 client script</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
    if (isLoading || newValue === '') {
        return;
    }

    var startBidAmount = g_form.getValue('item_name.start_bid_amount');
    var highestBidAmount = g_form.getValue('item_name.highest_bid_amount');
    var yourBidAmount = g_form.getValue('your_bid_amount');

    if (yourBidAmount <= highestBidAmount) {
        g_form.clearValue('your_bid_amount');
        g_form.addErrorMessage("Your bid amount should be greater than the current highest bid.");
        //         g_form.setValue('your_bid_amount', ''); // Clear the entered bid amount
    } else {
        g_form.setValue('item_name.highest_bid_amount', yourBidAmount); // Update the highest bid amount
    }
    //     if (yourBidAmount == highestBidAmount) {
    //         g_form.addErrorMessage("Error");
    //         g_form.setValue('your_bid_amount', '0.00');
    //     }

}]]></script>
