        <condition/>
        <description>onLoad the location field gets auto filled by the user location</description>
        <field>location</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>Location auto fetch</name>
        <order/>
        <script><![CDATA[function onLoad(control, oldValue, newValue, isLoading) {
    if (isLoading || newValue == '') {
        return;
    }
    var FetchLocationSI = Class.create();
    FetchLocationSI.prototype = Object.extendsObject(AbstractAjaxProcessor, {
        getUserLocation: function(userId) {
            var location = '';
            var userGr = new GlideRecord('sys_user');
            if (userGr.get(userId)) {
                location = userGr.getValue('location');
            }
            return location;
        },

        type: 'FetchLocationSI'
    });


}]]></script>
