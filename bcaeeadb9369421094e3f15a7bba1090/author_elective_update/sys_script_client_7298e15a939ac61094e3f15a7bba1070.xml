    <sys_script_client action="DELETE">
        <active>true</active>
        <applies_extended>false</applies_extended>
        <condition/>
        <description>Your bid amount should be greater than the current bid amount for an item on bid</description>
        <field>your_bid_amount</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>Validate bid amount</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
    if (isLoading || newValue === '') {
        return;
    }
    var currentBidAmount = g_form.getValue('current_bid_amount');
    if (parseFloat(newValue) <= parseFloat(currentBidAmount)) {
        g_form.showFieldMsg('your_bid_amount', 'Your bid amount must be greater than the current bid amount.', 'error');
        g_form.setValue('your_bid_amount', '');
    } else {
        g_form.hideFieldMsg('your_bid_amount', 'error');
    }
}]]></script>
