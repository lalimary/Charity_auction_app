<?xml version="1.0" encoding="UTF-8"?><record_update sys_domain="global" table="sys_script_client">
    <sys_script_client action="DELETE">
        <active>true</active>
        <applies_extended>false</applies_extended>
        <condition/>
        <description>The script hides the current bidding amount if the auction status is new.</description>
        <field/>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>Hide Current Bid Amount</name>
        <order/>
        <script><![CDATA[function onLoad() {
    var auctionStatus = g_form.getValue('auction_status');
    var currentBidAmountField = g_form.getControl('current_bid_amount');

    if (auctionStatus == 'item_submitted' || auctionStatus == 'auction_scheduled') {
        // Hide the Current Bidding Amount field
        currentBidAmountField.style.display = 'none';
    } else {
        // Show the Current Bidding Amount field
        currentBidAmountField.style.display = 'block';
    }
}
]]></script>
