        <messages/>
        <name>Display anonymous bidder</name>
        <order/>
        <script><![CDATA[function onLoad() {
    // Get the current user's user ID
    var currentUserId = g_user.userID;
    
    // Get the user ID of the bid creator
    var creatorId = g_form.getValue('sys_created_by');
    
    // If the current user is not the creator, show 'Anonymous' in the display name field
    if (currentUserId !== creatorId) {
        g_form.setValue('bidder', 'Anonymous');
    }
}
]]></script>
