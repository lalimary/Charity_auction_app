        <messages/>
        <name>Set Highest bid same as start bid </name>
        <order/>
        <script><![CDATA[function onLoad() {
    // Get the values of start_bid_amount and highest_bid_amount
    var startBidAmountStr = g_form.getValue('item_name.start_bid_amount');
    var highestBidAmountStr = g_form.getValue('item_name.highest_bid_amount');

    // Parse the currency values, remove any formatting if needed
    var startBidAmount = parseCurrency(startBidAmountStr);
    var highestBidAmount = parseCurrency(highestBidAmountStr);

    // Check if highest_bid_amount is less than start_bid_amount
    if (highestBidAmount < startBidAmount) {
        // Set highest_bid_amount to start_bid_amount
        g_form.setValue('item_name.highest_bid_amount', startBidAmountStr);
    }
}]]></script>
