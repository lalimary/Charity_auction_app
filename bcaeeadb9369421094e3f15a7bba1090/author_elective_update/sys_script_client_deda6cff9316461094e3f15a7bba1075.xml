        <name>CS3 (Test)</name>
        <order/>
        <script><![CDATA[function onLoad() {
    if (g_form.isNewRecord()) {
        var itemSysId = g_form.getValue('item_name');

        // GlideAjax to get the highest bid from the item table
        var ga = new GlideAjax('GetHighestBid');
        ga.addParam('sys_id', itemSysId);
        ga.getXMLAnswer(function(response) {
            var highestBid = response;
            g_form.setValue('item_name.highest_bid_amount', highestBid);
        });
    }
}

// Server-side script for GlideAjax
var GetHighestBid = Class.create();
GetHighestBid.prototype = Object.extendsObject(AbstractAjaxProcessor, {
    getHighestBid: function() {
        var itemSysId = this.getParameter('sys_id');
        var highestBid = 0;

        var item = new GlideRecord('x_1383184_charityb_item');
        if (item.get(itemSysId)) {
            highestBid = item.getValue('highest_bid_amount');
        }

        return highestBid.toString();
    }
});]]></script>
