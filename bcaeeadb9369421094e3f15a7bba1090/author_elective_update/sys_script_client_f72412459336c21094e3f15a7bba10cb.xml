        <name>Location field default value </name>
        <order/>
        <script><![CDATA[// Client script to auto-populate location if empty on user profile form load
(function onLoad() {
    // Check if it's a new record or the location field is empty
    if (g_form.isNewRecord() || !g_form.getValue('location')) {
        // Fetch the current user's location from the User table
        var currentUser = new GlideRecord('sys_user');
        if (currentUser.get(g_user.userID)) {
            // Get the location reference field value from the User record
            var locationRef = currentUser.getReferenceValue('location');

            // Set the location value on the form
            g_form.setValue('location', locationRef);
        }
    }
})();]]></script>
