    <sys_script_client action="DELETE">
        <active>false</active>
        <applies_extended>false</applies_extended>
        <condition/>
        <description/>
        <field>your_bid_amount</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>script 2</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
    if (isLoading || newValue === '') {
        return;
    }

    var startBidAmount = parseFloat(g_form.getValue('item_name.start_bid_amount'));
    var highestBidAmount = parseFloat(g_form.getValue('item_name.highest_bid_amount'));
    var yourBidAmount = parseFloat(newValue);

    if (isNaN(startBidAmount) || isNaN(yourBidAmount)) {
        g_form.addErrorMessage("Invalid bid amount.");
        return;
    }

    if (isNaN(highestBidAmount) || highestBidAmount < startBidAmount) {
        highestBidAmount = startBidAmount;
        g_form.setValue('highest_bid_amount', startBidAmount);
    }

    if (yourBidAmount <= highestBidAmount) {
        g_form.addErrorMessage("Your bid amount should be greater than the current highest bid.");
        g_form.setValue('your_bid_amount', '');
    }
}
]]></script>
