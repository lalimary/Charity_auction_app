        <script><![CDATA[(function() {
    // Get the current form
    var currentForm = g_form.getTableName();

    // Define the UI action function
    function uploadImage() {
        // Open the attachment dialog to select a file
        g_form.addAttachment(currentForm, g_form.getUniqueValue(), 'image', 'Upload Image', function(attachmentSysId) {
            // Attachment added successfully
            alert('Image uploaded successfully!');
        }, function(errorMsg) {
            // Error handling if attachment fails
            alert('Error uploading image: ' + errorMsg);
        });
    }

    // Execute the UI action function
    uploadImage();
})();
