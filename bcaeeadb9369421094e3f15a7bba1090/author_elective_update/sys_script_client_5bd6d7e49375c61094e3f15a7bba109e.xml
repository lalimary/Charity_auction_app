        <messages/>
        <name>Event start date from today</name>
        <order/>
        <script><![CDATA[function onChange() {
    // Get the start date field value
    var startDate = g_form.getValue('start_date');
 
    // Get the current date
    var currentDate = new Date();
 
    // Convert start date to Date object
    var startDateObj = new Date(startDate);
 
    // Validate start date is not before today
    if (startDateObj <= currentDate) { // Change to <= to include today
		g_form.clearValue('start_date');
        g_form.showFieldMsg('start_date', 'Start date cannot be today or before today', 'error');
        return;
    }

    // Clear the start date field value only if there's an error message
   

    // Clear any existing error message if the start date is valid
    g_form.clearMessages('start_date');
}]]></script>
